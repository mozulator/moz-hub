    "shooting",
    "precision",
    "mil-dot",
    "scope",
    "tarkov",
    "gaming",
    "prisma",
    "database"
  ],
  "author": "Sniper Buddy Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/sniper-buddy.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/sniper-buddy/issues"
  },
  "homepage": "https://github.com/your-username/sniper-buddy#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.43",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "prisma": "^5.7.1",
    "tailwindcss": "^4.1.11"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "cors": "^2.8.5",
