  "description": "Tactical ballistics calculator for precision shooting with dynamic weapon configurations and real-time calculations",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "serve-static": "python -m http.server 8080",
